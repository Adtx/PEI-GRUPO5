<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Result" table="Result" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="Designation" column="Designation" type="string" length="255" not-null="false" lazy="false"/>
		<one-to-one name="response" class="Response" cascade="save-update,lock" property-ref="result" lazy="proxy" access="field"/>
	</class>
</hibernate-mapping>
