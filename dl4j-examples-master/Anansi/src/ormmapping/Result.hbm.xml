<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="hibernate.Result" table="Result" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="response" column="ResponseID" class="hibernate.Response" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="Designation" column="Designation" type="string" length="255" not-null="true" lazy="false"/>
		<property name="result" column="Result" type="double" not-null="false" lazy="false"/>
		<set name="ORM_ComponentResults" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="ResultID" not-null="false"/>
			<one-to-many class="hibernate.ComponentResult"/>
		</set>
	</class>
</hibernate-mapping>
